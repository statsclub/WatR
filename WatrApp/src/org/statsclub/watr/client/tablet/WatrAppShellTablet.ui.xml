<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:SplitLayoutPanel>
		<g:west size='200'>
			<g:StackLayoutPanel unit='PX'>
				<g:stack>
					<g:customHeader size='50'>
						<g:Label>Project</g:Label>
					</g:customHeader>
					<g:Label>data01</g:Label>
				</g:stack>
				<g:stack>
					<g:customHeader size='50'>
						<g:Label>Data</g:Label>
					</g:customHeader>
					<g:Label>data01</g:Label>
				</g:stack>
				<g:stack>
					<g:customHeader size='50'>
						<g:Label>Model</g:Label>
					</g:customHeader>
					<g:Label>data01</g:Label>
				</g:stack>
				<g:stack>
					<g:customHeader size='50'>
						<g:Label>Graph</g:Label>
					</g:customHeader>
					<g:Label>data01</g:Label>
				</g:stack>
				<g:stack>
					<g:customHeader size='50'>
						<g:Label>Table</g:Label>
					</g:customHeader>
					<g:Label>data01</g:Label>
				</g:stack>
			</g:StackLayoutPanel>
		</g:west>
		<g:center>
			<g:SimpleLayoutPanel ui:field="outputPanel"/>			
		</g:center>
		<g:east size='200'>
			<g:DisclosurePanel>
				<g:customHeader>
					<g:Label>Windows</g:Label>
				</g:customHeader>
				<g:Label>Settings</g:Label>
			</g:DisclosurePanel>
		</g:east>
		<g:south size='200'>
			<g:DockLayoutPanel unit='PX'>
				<g:center>
					<g:SimpleLayoutPanel />
				</g:center>
				<g:south size='30'>
					<g:DockLayoutPanel unit='PX'>
						<g:center>
							<g:TextBox />
						</g:center>
						<g:east size='100'>
							<g:Button>Enter</g:Button>
						</g:east>
					</g:DockLayoutPanel>
				</g:south>
			</g:DockLayoutPanel>
		</g:south>
	</g:SplitLayoutPanel>
</ui:UiBinder> 